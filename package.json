{
  "name": "dragd-monolith",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "concurrently \"npm run moser\" \"next start\"",
    "lint": "next lint",
    "dragd": "next dev",
    "dev": "concurrently \"npm run dragd\" \"npm run moser\"",
    "moser": "moser -p 3001 --mongo `(grep MONGO_ .env.local | cut -d '=' -f2)` || true",
    "moser-build": "moser -p 3001 --mongo ",
    "build-static": "APP_MODE=static next build",
    "export": "APP_MODE=static concurrently \"npm run moser\" \"next export\"",
    "builder": "concurrently \"npm run moser\" \"node server.js\""
  },
  "dependencies": {
    "@ensdomains/ensjs": "^3.0.0-alpha.64",
    "@giphy/js-fetch-api": "^4.7.1",
    "@giphy/react-components": "^6.8.1",
    "@monaco-editor/react": "^4.4.6",
    "@next/font": "13.2.4",
    "@react-spring/web": "^9.7.2",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "concurrently": "^7.6.0",
    "daisyui": "^2.51.5",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "ethers": "^5.7.2",
    "iron-session": "^6.3.1",
    "js-yaml": "^4.1.0",
    "moser": "^0.7.0",
    "next": "13.2.4",
    "node-cron": "^3.0.2",
    "react": "18.2.0",
    "react-async-hook": "^4.0.0",
    "react-audio-player": "^0.17.0",
    "react-colorful": "^5.6.1",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.6",
    "react-player": "^2.12.0",
    "react-query": "^3.39.3",
    "react-use": "^17.4.0",
    "rehype-raw": "^6.1.1",
    "siwe": "^2.1.3",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0",
    "w3name": "^1.0.8",
    "wagmi": "^0.12.6",
    "web3.storage": "^4.5.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7"
  }
}
